<h1>Sign up!</h1>

<%= form_for(@user) do |f| %>
	
	<%= f.label :name, "Name" %>
	<%= f.text_field :name %>

	<%= f.label :email, "Email" %>
	<%= f.text_field :email %>

	<%= f.label :password, "Password" %>
	<%= f.password_field :password %>

	<%= f.label :password_confirmation, "Confirmation" %>
	<%= f.password_field :password_confirmation %>

	<%= f.submit "Sign me up" %>

<% end %>